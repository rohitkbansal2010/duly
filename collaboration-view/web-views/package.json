    "chartjs-adapter-date-fns": "^2.0.0",
    "chartjs-plugin-annotation": "^1.2.2",
    "chartjs-plugin-zoom": "^1.2.0",
    "connected-react-router": "^6.9.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.22.1",
    "dotenv-webpack": "^7.0.3",
    "enzyme": "^3.11.0",
    "glob-parent": "^6.0.0",
    "html2canvas": "^1.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-rc.0",
    "react-bootstrap-typeahead": "^6.0.0-alpha.11",
    "react-chartjs-2": "^3.0.5",
    "react-datepicker": "^4.1.1",
    "react-detect-click-outside": "^1.1.1",
    "react-dom": "^17.0.2",
    "react-gateway": "^3.0.0",
    "react-html2pdf": "^1.0.1",
    "react-lottie": "^1.2.3",
    "react-range": "^1.8.9",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "swiper": "^8.0.3",
    "usehooks-ts": "^1.0.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.3.12",
    "@storybook/react": "^6.4.9",
    "@testing-library/react": "^13.0.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.4",
    "@types/react-datepicker": "^3.1.8",
    "@types/react-dom": "^17.0.3",
    "@types/react-lottie": "^1.2.6",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^17.0.1",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
    "babel-jest": "^27.2.5",
    "babel-loader": "^8.2.2",
    "canvas": "^2.9.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "del-cli": "^4.0.1",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.25.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.2.5",
    "jest-dom": "^4.0.0",
    "jest-junit": "^13.0.0",
    "jest-localstorage-mock": "^2.4.10",
    "jest-sonar-reporter": "^2.0.0",
    "jsdom": "18.0.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "^10.5.4",
    "node-sass": "^6.0.1",
    "postcss-scss": "^4.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "prettier-plugin-style-order": "^0.2.2",
    "react-test-renderer": "^17.0.2",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "thread-loader": "^3.0.4",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.7.3"
  },
  "pre-commit": {
    "silent": true,
    "run": [
      "lint:fix",
      "format",
      "stage:all",
      "test",
      "build"
    ]
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@mock-data(.*)$": "<rootDir>/src/mock-data$1",
      "^@enums(.*)$": "<rootDir>/src/common/enums$1",
      "^@hooks(.*)$": "<rootDir>/src/common/hooks$1",
      "^@localization(.*)$": "<rootDir>/src/localization$1",
      "^@pages(.*)$": "<rootDir>/src/pages$1",
      "^@redux/(.*)$": "<rootDir>/src/redux/$1",
      "^@sagas(.*)$": "<rootDir>/src/sagas$1",
      "^@routes(.*)$": "<rootDir>/src/routes$1",
      "^@utils(.*)$": "<rootDir>/src/common/utils$1",
      "^@icons(.*)$": "<rootDir>/src/common/icons$1",
      "^@constants(.*)$": "<rootDir>/src/common/constants$1",
      "^@images(.*)$": "<rootDir>/src/common/images$1",
      "^@styles(.*)$": "<rootDir>/src/common/styles$1",
      "^@types(.*)$": "<rootDir>/src/common/types$1",
      "^@ui-kit(.*)$": "<rootDir>/src/ui-kit$1",
      "^@interfaces(.*)$": "<rootDir>/src/common/interfaces$1",
      "^@http(.*)$": "<rootDir>/src/http$1",
      ".+\\.(css|styl|sass|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      "\\.(png|svg)$": "<rootDir>/__mocks__/fileTransformer.js"
    },
    "testRegex": "\\.test\\.tsx?$",
    "setupFiles": [
      "<rootDir>/setupJest.ts"
    ],
    "coverageReporters": [
      "lcov",
      "cobertura"
    ],
    "collectCoverageFrom": [
      "src/*.{ts,tsx}",
      "src/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/@types",
      "<rootDir>/src/common/constants",
      "<rootDir>/src/common/enums",
      "<rootDir>/src/common/icons",
      "<rootDir>/src/common/interfaces",
      "<rootDir>/src/common/styles",
      "<rootDir>/src/common/types",
      ".stories.@(js|jsx|ts|tsx)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "jestSonar": {
    "reportPath": "testResults",
    "reportFile": "sonar-report.xml",
    "indent": 4
  }
}
